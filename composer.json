{
    "name": "isovera/isovera-profile",
    "description": "Drupal 8 install profile based on Isovera's standards.",
    "type": "drupal-profile",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Travis Tomka",
            "email": "underoathosx@gmail.com"
        }
    ],
    "repositories": {
        "drupal": {
            "type": "composer",
            "url":  "https://packages.drupal.org/8"
        },
        "isotheme_starter": {
            "type": "vcs",
            "url": "git@github.com:isovera/isotheme_starter.git",
            "no-api": true
        }
    },
    "require": {
        "drupal/devel": "^1.0",
        "drupal/token": "^1.0",
        "drupal/seckit": "^1.0",
        "drupal/linkit": "^4.0",
        "drupal/ctools": "^3.0",
        "drupal/webform": "^5.0",
        "drupal/metatag": "^1.0",
        "drupal/redirect": "^1.0",
        "drupal/pathauto": "^1.0",
        "drupal/paragraphs": "^1.1",
        "drupal/field_group": "^1.0",
        "drupal/config_split": "^1.0",
        "drupal/admin_toolbar": "^1.0",
        "drupal/module_filter": "^3.0",
        "drupal/config_update": "^1.3",
        "drupal/simple_sitemap": "^2.9",
        "drupal/password_policy": "^3.0",
        "drupal/google_analytics": "^2.1",
        "drupal/stage_file_proxy": "^1.0",
        "isovera/isotheme_starter": "^0.0.1",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/editor_advanced_link": "^1.4",
        "drupal/adminimal_admin_toolbar": "^1.3",
        "drupal/entity_reference_revisions": "^1.2"
    }
}
